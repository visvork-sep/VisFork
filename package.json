{
  "name": "visfork-plugin",
  "version": "0.0.0",
  "type": "module",
  "description": "visvork-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/fasonju/visfork-plugin.git"
  },
  "scripts": {
    "generate": "graphql-codegen && openapi-typescript ./openapi-schema.yaml -o ./__generated__/rest-schema.d.ts",
    "dev": "vite --host 127.0.0.1 --port 3000",
    "build": "npm run generate && tsc -b && vite build",
    "fix-lint": "eslint . --quiet --fix",
    "lint": " eslint . --max-warnings 0",
    "test": "npm run test-coverage",
    "test-coverage": "jest --forceExit --coverage --verbose",
    "test-slim": "jest --forceExit --passWithNoTests"
  },
  "author": "Ren Fu",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@graphql-codegen/cli": "^5.0.4",
    "@graphql-codegen/client-preset": "^4.6.2",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.20.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^15.15.0",
    "graphql-config": "^5.1.3",
    "jest": "^29.7.0",
    "openapi-typescript": "^7.6.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.1.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@primer/octicons-react": "^19.15.0",
    "@primer/react": "^37.13.0",
    "@tanstack/react-query": "^5.66.0",
    "graphql": "^16.10.0",
    "graphql-request": "^7.1.2",
    "openapi-fetch": "^0.13.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "styled-components": "^5.3.7"
  }
}
